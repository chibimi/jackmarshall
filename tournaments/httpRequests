# $endpoint = http://localhost:8080
# $id = 

# GetAllScenario
GET $endpoint/scenario

# GetOneScenario
GET $endpoint/scenario/$id

# CreateScenario
POST $endpoint/scenario
{
  "name":"Devastation",
  "year":2016,
  "link":"http://google.com"
}

# UpdateScenario
PUT $endpoint/scenario/$id
{
  "name":"Devastation",
  "year":2016,
  "link":"http://google.fr"
}

# DeleteScenario
DELETE $endpoint/scenario/$id
